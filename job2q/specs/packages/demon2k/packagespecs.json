{
   "shortname": "deMon",
   "packagename": "deMon2k",
   "parallelib": "intelmpi",
   "mpilaunch": "True",

   "conflicts": {
      "not inp": "Debe existir un archivo inp"
   },

   "defaults" : {
      "jobdir": "True"
   },

   "filekeys": {
      "inp": "deMon.inp",
      "inp.new": "deMon.inp",
      "rst": "deMon.rst",
      "trj": "deMon.trj",
      "oldtrj": "deMon.trj",
      "out": "deMon.out",
      "mol": "deMon.mol",
      "new": "deMon.new",
      "mem": "deMon.mem",
      "pie": "deMon.pie",
      "bin": "deMon.bin"
   },

   "fileoptions": {
      "rstfile": "rst",
      "trjfile": "oldtrj"
   },

   "inputfiles": [
      "inp",
      "rst",
      "oldtrj"
   ],

   "outputfiles": [
      "inp.new",
      "trj",
      "out",
      "mol",
      "new",
      "mem",
      "pie",
      "bin"
   ],

   "interpolable": [
      "inp"
   ],

   "parameters": [
      "bases"
   ]
}
