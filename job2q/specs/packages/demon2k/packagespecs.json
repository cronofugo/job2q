{
   "packagefix": "deMon",
   "packagename": "deMon2k",
   "parallelib": "intelmpi",
   "mpilaunch": "True",
   "filecheck": "inp",
   "defaults" : {
      "outdir": "{job}"
   },
   "filekeys": {
      "inp": "deMon.inp",
      "modinp": "deMon.inp",
      "rst": "deMon.rst",
      "trj": "deMon.trj",
      "oldtrj": "deMon.trj",
      "out": "deMon.out",
      "mol": "deMon.mol",
      "new": "deMon.new",
      "mem": "deMon.mem",
      "pie": "deMon.pie",
      "bin": "deMon.bin"
   },
   "fileopts": {
      "rstfile": "rst",
      "trjfile": "oldtrj"
   },
   "infiles": [
      "inp",
      "rst",
      "oldtrj"
   ],
   "outfiles": [
      "modinp",
      "trj",
      "out",
      "mol",
      "new",
      "mem",
      "pie",
      "bin"
   ],
   "interpolable": [
      "inp"
   ],
   "keywords": [
      "title",
      "method",
      "basis",
      "auxis",
      "ecps",
      "charge",
      "spin"
   ],
   "parameters": [
      "bases"
   ]
}
