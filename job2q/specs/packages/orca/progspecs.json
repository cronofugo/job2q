{
   "progkey": "ORCA",
   "progname": "ORCA",
   "parallelib": "openmpi",
   "mpilaunch": "False",
   "filecheck": "inp",
   "stdoutput": "out",
   "defaults" : {
      "jobdir": "{jobname}"
   },
   "filekeys": {
      "inp": "job.inp",
      "out": "job.out",
      "opt": "job.opt",
      "xyz": "job.xyz",
      "trj": "job.trj",
      "gbw": "job.gbw",
      "prop": "job.prop",
      "engrad": "job.engrad",
      "property": "job_property.txt"
   },
   "filevars": {
      "inp": "inp"
   },
   "inputfiles": [
      "inp"
   ],
   "positionargs": [
      "inp"
   ],
   "outputfiles": [
      "inp",
      "gbw",
      "out",
      "opt",
      "xyz",
      "trj",
      "prop",
      "engrad",
      "property"
   ],
   "prescript": [
      "sed -i '/\\s*%pal\\s\\s*end/Id;1i'\"%pal nprocs $nproc end\" $inp"
   ]
}
