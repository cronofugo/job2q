{
   "progkey": "DFTB+",
   "progname": "DFTB+",
   "parallelib": "openmp",
   "filecheck": "dftb_in.hsd",
   "stdoutput": "dftb_out.log",
   "defaults": {
      "outdir": "{jobname}.{progkey}{version}"
   },
   "filekeys": {
      "dftb_in.hsd": "dftb_in.hsd",
      "dftb_pin.hsd": "dftb_pin.hsd",
      "dftb_out.log": "dftb_out.log",
      "charges.bin": "charges.bin",
      "geo_end.xyz": "geo_end.xyz",
      "geo_end.gen": "geo_end.gen",
      "results.tag": "results.tag",
      "detailed.out": "detailed.out",
      "detailed.xml": "detailed.xml",
      "autotest.tag": "autotest.tag",
      "hessian.out": "hessian.out",
      "band.out": "band.out",
      "md.out": "md.out",
      "EXC.DAT": "EXC.DAT",
      "TRA.DAT": "TRA.DAT",
      "SPX.DAT": "SPX.DAT",
      "XCH.DAT": "XCH.DAT",
      "XREST.DAT": "XREST.DAT",
      "COEF.DAT": "COEF.DAT",
      "XplusY.DAT": "XplusY.DAT",
      "TDP.DAT": "TDP.DAT",
      "ARPACK.DAT": "ARPACK.DAT",
      "TEST_ARPACK.DAT": "TEST_ARPACK.DAT"
   },
   "filevars": {
      "dftbin": "dftb_in.hsd"
   },
   "realfiles": {
      "charges": "charges.bin"
   },
   "inputfiles": [
      "dftb_in.hsd",
      "charges.bin"
   ],
   "outputfiles": [
      "dftb_in.hsd",
      "charges.bin",
      "dftb_pin.hsd",
      "dftb_out.log",
      "geo_end.xyz",
      "geo_end.gen",
      "results.tag",
      "detailed.out",
      "detailed.xml",
      "autotest.tag",
      "hessian.out",
      "band.out",
      "md.out",
      "EXC.DAT",
      "TRA.DAT",
      "SPX.DAT",
      "XCH.DAT",
      "XREST.DAT",
      "COEF.DAT",
      "XplusY.DAT",
      "TDP.DAT",
      "ARPACK.DAT",
      "TEST_ARPACK.DAT"
   ],
   "parametersets": [
      "slako"
   ],
   "prescript": [
      "sed -ri 's,(^|[{;])\\s*OutputPrefix\\s*=\\s*\"[^\"]*\"\\s*($|[;}]),\\1\\2,I' $dftbin"
   ]
}
