{
   "usempilauncher": "False",
   "packagekey": "orca",
   "packagename": "ORCA",
   "prescript": [
      "sed -i '/\\s*%pal\\s\\s*end/Id;1i'\"%pal nprocs $ncore end\" $inp"
   ],
   "filecheck": "inp",
   "parallelization": "openmpi",
   "filevars": {
      "inp": "inp"
   },
   "outputfiles": [
      "inp",
      "gbw",
      "out",
      "opt",
      "xyz",
      "trj",
      "prop",
      "engrad",
      "property"
   ],
   "positionargs": [
      "inp"
   ],
   "stdout": "out",
   "makejobdir": "True",
   "inputfiles": [
      "inp"
   ],
   "filenames": {
      "xyz": "job.xyz",
      "gbw": "job.gbw",
      "out": "job.out",
      "engrad": "job.engrad",
      "opt": "job.opt",
      "prop": "job.prop",
      "property": "job_property.txt",
      "trj": "job.trj",
      "inp": "job.inp"
   }
}