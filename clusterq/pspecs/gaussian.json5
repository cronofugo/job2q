{
   parallel: 'omp',

   conflicts: {
      'com and gjf': 'Conflicto entre los archivos de entrada $file.com y $file.gjf',
      'not com and not gjf': 'Debe existir un archivo de entrada con extensi√≥n com o gjf',
   },

   filekeys: {
      com: 'job.com',
      gjf: 'job.gjf',
      log: 'job.log',
      chk: 'job.chk',
      fchk: 'job.fchk',
      wfn: 'job.wfn',
      cub: 'job.cub',
      oldchk: 'job.oldchk',
   },

   filevars: {
      comfile: 'com',
      gjffile: 'gjf',
      chkfile: 'chk',
      fchkfile: 'fchk',
      oldchkfile: 'oldchk',
   },

   fileoptions: {
      oldchk: 'oldchk',
   },

   inputfiles: [
      'com',
      'gjf',
      'oldchk',
   ],

   outputfiles: [
      'log',
      'chk',
      'fchk',
      'wfn',
      'cub',
   ],

   interpolable: [
      'gjf',
      'com',
   ],

   interpolationoptions: [
      'method',
      'basis',
      'auxis',
      'charge',
      'multiplicity',
   ],

   posargs: [
      'gjf|com',
   ],

   prescript: [
      'sed -i "/^%/d" @($gjffile|$comfile)',
      'sed -i "1i%Chk=$chkfile\\\\n%NProcShared=${nproc}\\\\n%Mem=${jobram}MB" @($gjffile|$comfile)',
      'sed -i "/--Link1--/a%Chk=$chkfile\\\\n%NProcShared=${nproc}\\\\n%Mem=${jobram}MB" @($gjffile|$comfile)',
      'test -f $oldchkfile && sed -i "1i%OldChk=$oldchkfile" @($gjffile|$comfile)',
   ],

}
